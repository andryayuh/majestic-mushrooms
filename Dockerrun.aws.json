{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "jenniferwg/timebox:app",
      "environment": [],
      "essential": true,
      "memory": 128,
      "portMappings": [{
        "hostPort": 3030,
        "containerPort": 3030
      }],
      "links": [
        "redis"
      ],
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-app",
          "containerPath": "/var/log/app"
        }
      ]
    },
    {
      "name": "redis",
      "image": "redis:alpine",
      "essential": true,
      "memory": 128,
      "portMappings": [{
        "hostPort": 3032,
        "containerPort": 6379
      }],
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-redis",
          "containerPath": "/var/log/redis"
        },
      ]
    }
  ]
}